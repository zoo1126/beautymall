<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
   <validator>
   <validator-id>pwdvalidator</validator-id>
   <validator-class>log_and_reg.pwdvalidator</validator-class>
   </validator>
      <validator>
   <validator-id>idvalidator</validator-id>
   <validator-class>log_and_reg.idvalidator</validator-class>
   </validator>
         <validator>
   <validator-id>advalidator</validator-id>
   <validator-class>log_and_reg.advalidator</validator-class>
   </validator>
   <validator>
   <validator-id>telvalidator</validator-id>
   <validator-class>log_and_reg.telvalidator</validator-class>
   </validator>
<navigation-rule>

<from-view-id>/login.xhtml</from-view-id>
<navigation-case>
<from-outcome>logincheck</from-outcome>
<to-view-id>/NewFile.xhtml</to-view-id>
<redirect/>
</navigation-case>
</navigation-rule>

</faces-config>
